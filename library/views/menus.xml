<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Top menu item -->
        <!-- Olaf: here the icon must be specified, it is NOT sufficient to just provide the file in the folder! -->
        <menuitem
            name="Library"
            id="menu_root"
            web_icon="library,static/description/icon.png"
        />
        <!-- menu categories -->
        <menuitem name="Rental" id="menu_rental_top" parent="menu_root"/>
        <menuitem name="Customers" id="menu_customer_top" parent="menu_root"/>
        <menuitem name="Books" id="menu_book_top" parent="menu_root"/>
        <menuitem name="Configuration" id="menu_configuration_top" parent="menu_root"/>
    </data>
</odoo>
